%ignore " " | /\t/

exp			: label 
			| arr_label
			| add
			| sub
			| mov
			| jz

arr_label	: label ".space 160"
label		: /[0-9a-zA-Z_]+/ ":"

add			: "add" suffix operand "," operand
sub			: "sub" suffix operand "," operand
mov			: "mov" suffix operand "," operand
jz			: "jz"  /[0-9a-zA-Z_]+/

suffix		: /q/ 
			| /l/

operand		: x64register
			| x32register
			| name
			| dummy
			| immidiate

x64register	: /%rax/
			| /%rdx/

x32register	: /%eax/
			| /%edx/

name		: /[0-9a-zA-Z]+/
dummy		: /R[0-9]+\+[0-9]+\*8\(%rip\)/
immidiate	: "$" /[+-]?\d+/